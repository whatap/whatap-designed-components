{
  "name": "whatap-designed-components",
  "version": "0.7.5",
  "description": "Designed components used at WhaTap",
  "main": "src/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "umd/",
    "es/"
  ],
  "scripts": {
    "init:jest": "jest --init",
    "test:jest": "jest",
    "test:assert": "npm run build:es5_only && node ./es/test/ColorPicker.test.js && node ./es/test/Palette.test.js && node ./es/test/HeapSort.test.js && node ./es/test/QuickSort.test.js",
    "build:umd_only": "webpack",
    "build:es5_only": "babel src --out-dir es",
    "build:demo": "babel src --out-dir ../wdc-demo/src/wdc",
    "build:whatap": "babel src --out-dir ../apm/io.whatap/whatap.server.front.apm/src/main/resources/front/node_modules/whatap-designed-components/es",
    "build:all": "npm run test:assert && npm run build:umd_only && npm run build:es5_only",
    "publish:latest": "npm publish",
  "publish:alpha": "npm publish --tag alpha"
  },
  "author": "Anthony Lee",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "node-localstorage": "^1.3.1",
    "prop-types": "^15.6.2",
    "react": "^15.6.2",
    "regenerator-runtime": "^0.13.1",
    "style-loader": "^0.23.1",
    "typescript": "^3.1.6",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "babel-plugin-css-modules-transform": "^1.6.2",
    "classnames": "^2.2.6",
    "lodash.merge": "^4.6.1",
    "clean-webpack-plugin": "^0.1.19",
    "moment": "^2.22.2"
  },
  "peerDependencies": {
    "react": "^16.0.0-0",
    "react-dom": "^16.0.0-0"
  }
}

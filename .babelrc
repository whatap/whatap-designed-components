{
    "presets": [
      ["@babel/preset-react"],
      ["@babel/preset-env", {
        "targets": {
          "browsers": [ "last 1 version", "ie >= 11" ]
        }
      }],
      ["@babel/preset-typescript"]
    ],
    "plugins": [
      ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ]
}